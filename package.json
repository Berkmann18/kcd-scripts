{
  "name": "mb-scripts",
  "version": "0.0.0-semantically-released",
  "description": "CLI for common scripts for my projects",
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "bin": {
    "mb-scripts": "dist/index.js"
  },
  "scripts": {
    "sec": "snyk test",
    "test": "node src test",
    "test:update": "node src test --updateSnapshot",
    "build": "node src build",
    "lint": "node src lint",
    "format": "node src format",
    "validate": "node src validate",
    "prepare": "snyk protect"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node src pre-commit"
    }
  },
  "files": [
    "dist",
    "babel.js",
    "eslint.js",
    "config.js",
    "prettier.js",
    "jest.js"
  ],
  "keywords": [
    "mb",
    "scripts",
    "common",
    "berkmann"
  ],
  "author": {
    "name": "Maximilian Berkmann",
    "email": "maxieberkmann@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.4",
    "arrify": "^2.0.1",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^24.8.0",
    "babel-plugin-macros": "^2.5.1",
    "babel-plugin-minify-dead-code-elimination": "^0.5.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "browserslist": "^4.6.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.1.4",
    "cross-spawn": "^6.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "doctoc": "^1.4.0",
    "eslint": "^5.16.0",
    "eslint-config-kentcdodds": "^14.3.2",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-jquery": "^1.5.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.5.0",
    "glob": "^7.1.4",
    "husky": "^2.3.0",
    "is-ci": "^2.0.0",
    "jest": "^24.8.0",
    "jest-watch-typeahead": "^0.3.1",
    "lint-staged": "^8.1.6",
    "lodash.camelcase": "^4.3.0",
    "lodash.has": "^4.5.2",
    "lodash.omit": "^4.5.0",
    "mkdirp": "^0.5.1",
    "prettier": "^1.17.1",
    "read-pkg-up": "^6.0.0",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-consistent": "^2.0.2",
    "remark-preset-lint-recommended": "^3.0.2",
    "resolve": "^1.10.1",
    "rimraf": "^2.6.2",
    "rollup": "^1.11.3",
    "rollup-plugin-babel": "^4.0.0",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.8.0",
    "rollup-plugin-terser": "^4.0.4",
    "semver": "^6.0.0",
    "snyk": "^1.173.1",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "which": "^1.3.0",
    "yargs-parser": "^13.1.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:security/recommended",
      "plugin:you-dont-need-lodash-underscore/compatible",
      "plugin:node/recommended"
    ],
    "rules": {
      "no-process-exit": "off",
      "no-console": "off"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Berkmann18/mb-scripts.git"
  },
  "bugs": {
    "url": "https://github.com/Berkmann18/mb-scripts/issues"
  },
  "homepage": "https://github.com/Berkmann18/mb-scripts#readme",
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "jest-in-case": "^1.0.2",
    "slash": "^3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
